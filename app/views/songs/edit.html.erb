<h1> Edit Song </h1>
<br>
<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :artist_id %>
  <%= f.text_field :artist_id %>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id %>
  <%= f.submit %>
<% end %>

<br>
<%= link_to "Cancel", song_path(@song) %>
